{
  "name": "placedai",
  "version": "1.0.0",
  "description": "AI-Powered Recruitment Platform - Full Stack Application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd backend && npm run dev",
    "dev:client": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "seed": "cd backend && npm run seed"
  },
  "keywords": [
    "recruitment",
    "ai",
    "interview",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rakesh2604/PlaceAI.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/rakesh2604/PlaceAI/issues"
  },
  "homepage": "https://github.com/rakesh2604/PlaceAI#readme"
}
